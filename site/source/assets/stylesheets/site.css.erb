[class*=kickoff][readonly]{
  pointer-events: auto;
  opacity: 1;
}
[class*=kickoff].focus{
  outline: none !important;
}
.bc-primary{
  background-color: #1e87f0 !important;
}
.b-primary{
  background: linear-gradient(to left top, #28a5f5, #1e87f0) 0 0 no-repeat !important;
}
.c-primary{
  color: #1e87f0 !important;
}
.rounded{
  border-radius: 4px !important;
}
a:not(.btn){
  color: #1e87f0;
  transition: color 0.3s ease;
}
a:not(.btn):hover,
a:not(.btn):active{
  color: #28a5f5;
}
.btn{
  border-radius: 4px;
  transition: background-color 0.3s ease;
}
.btn.btn-primary{
  background: #1e87f0;
}
.btn.btn-primary:hover,
.btn.btn-primary:active,
.btn.btn-primary.btn-active{
  background: #28a5f5;
}
html{
  min-width: 1200px;
}
body::before{
  content: "";
  display: block;
  position: fixed;
  z-index: 0;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  background: url(<%= asset_url "/assets/images/background_compressed.png" %>) center no-repeat;
  background-size: cover;
  opacity: 0.4;
}
code{
  padding: 0.2em 0.5em;
  font-size: 0.8125em;
}
code,
.highlight,
.table,
.media .media-content,
iframe{
  border-radius: 4px;
}
.code{
  display: block;
  padding: 1rem;
  background: none;
}
.code code{
  padding-top: .597rem;
  padding-bottom: .403rem;
  font-size: 1.0833333333rem;
  line-height: 2rem;
}
.hollow-header{
  transition: background-color 0.5s ease, box-shadow 0.5s ease;
}
.hollow-header.hollow-header-show-background{
  background-color: #1e87f0;
  box-shadow: 0 0 10px rgba(0, 0, 0, 0.3);
}
.hollow-header-logo img{
  position: relative;
  top: 0.2rem;
  width: 12rem;
  height: 4rem;
}
.homepage-slogan{
  text-shadow: 0 0 3px rgba(0,0,0,0.3)
}
#my-kickoff{
  height: 652px;
}
@media (min-width: 1440px) {
  #doc{
    padding-right: 20rem;
  }
}
#doc-aside{
  width: 20rem;
  border-right: solid 1px #e5e5e5;
}
#doc-nav{
  position: sticky;
  top: 7rem;
  height: calc(100vh - 7rem);
}
#doc-nav h2{
  border-bottom: solid 1px #e5e5e5;
}
#doc-nav a{
  color: #8f8f8f;
  transition: color 0.3s ease;
}
#doc-nav a:hover,
#doc-nav a.doc-nav-current-link{
  color: #1e87f0;
}
#doc-article{
  max-width: 90rem;
  min-width: 0;
}
#doc-article > * {
  max-width: 62rem;
  margin-left: auto !important;
  margin-right: auto !important;
}
#doc-article > iframe {
  max-width: 100%;
}
#doc-article p{
  line-height: 2.5rem;
}



